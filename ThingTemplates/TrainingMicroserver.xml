                             baseType="STRING"
                             description="Metric for evaluating model performance within ensemble (PEARSONS,RMSE,ROC)"
                             name="comparisonMetric"
                             ordinal="7"></FieldDefinition>
                            <FieldDefinition
                             aspect.dataShape="NumberArrayElement"
                             baseType="INFOTABLE"
                             description="Each confidence level represents the probability that the returned confidence interval contains the true value. Each confidence level must be between 0 and 1, exclusive (Default is .80). Larger confidence levels yield wider confidence intervals."
                             name="confidenceLevels"
                             ordinal="17"></FieldDefinition>
                            <FieldDefinition
                             aspect.dataShape="AnalyticsConfusionMatrixWeightsParams"
                             baseType="INFOTABLE"
                             description="The ConfusionMatrixWeights gives more control over the selection of the boolean threshold when training a model with a boolean goal. It is a 2 by 2 matrix where each element is the relative weight associated with the respective quadrant of the confusion matrix."
                             name="confusionMatrixWeights"
                             ordinal="18"></FieldDefinition>
                            <FieldDefinition
                             aspect.defaultValue="false"
                             baseType="BOOLEAN"
                             description="If True, a confidence model is created (Default is False)."
                             name="createConfidenceModel"
                             ordinal="16"></FieldDefinition>
                            <FieldDefinition
                             aspect.dataShape="AnalyticsDatasetRef"
                             baseType="INFOTABLE"
                             description="Reference to the dataset for this job"
                             name="datasetRef"
                             ordinal="2"></FieldDefinition>
                            <FieldDefinition
                             baseType="STRING"
                             description="Optional description for this job"
                             name="description"
                             ordinal="1"></FieldDefinition>
                            <FieldDefinition
                             baseType="STRING"
                             description="Options for aggregating the learner outputs (ELITE_AVERAGE,AVERAGE,SOLOIST,BEST, MAJORITY_VOTE)"
