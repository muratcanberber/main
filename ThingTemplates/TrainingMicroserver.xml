                             name="learners"
                             ordinal="6"></FieldDefinition>
                            <FieldDefinition
                             aspect.defaultValue="1"
                             baseType="INTEGER"
                             description="For timeseries data, specify the number of time steps ahead to predict (cannot be negative)"
                             name="lookahead"
                             ordinal="9"></FieldDefinition>
                            <FieldDefinition
                             baseType="INTEGER"
                             description="For timeseries data, number of historical data points to consider (enter 0 for auto-windowing - affects runtime)"
                             name="lookbackSize"
                             ordinal="8"></FieldDefinition>
                            <FieldDefinition
                             aspect.defaultValue="25"
                             baseType="INTEGER"
                             description="Limit model to use a specified number of fields (affects runtime)"
                             name="maxAllowedFields"
                             ordinal="11"></FieldDefinition>
                            <FieldDefinition
                             aspect.defaultValue="1"
                             baseType="INTEGER"
                             description="For TEXT fields, the maximum size (in consecutive words) of n-grams (words or phrases) that can be used as features (affects runtime)"
                             name="maxNGramSize"
                             ordinal="20"></FieldDefinition>
                            <FieldDefinition
                             aspect.defaultValue="1000"
                             baseType="INTEGER"
                             description="For TEXT fields, the maximum number of different n-grams of each size that can be used as features"
                             name="maxVocabSize"
                             ordinal="21"></FieldDefinition>
                            <FieldDefinition
                             aspect.defaultValue="0.0"
                             baseType="NUMBER"
                             description="For TEXT fields, the minimum fraction of dataset rows in which an n-gram must appear in order to be used. Allowed values are 0.0 (inclusive) to 1.0 (exclusive)"
                             name="minDocumentFrequency"
                             ordinal="22"></FieldDefinition>
                            <FieldDefinition
                             baseType="INTEGER"
                             description="Optional number of folds to use for K-fold validation during model training. Cannot be used if a validationHoldoutPercentage is also specified."
                             name="numberOfFolds"
                             ordinal="23"></FieldDefinition>
                            <FieldDefinition
                             aspect.dataShape="AnalyticsSamplingParams"
                             baseType="INFOTABLE"
                             description="Parameters for upsampling and downsampling a boolean goal"
